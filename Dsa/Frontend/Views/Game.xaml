<Page x:Class="Frontend.Views.Game"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Frontend.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Game">

    <StackPanel Orientation="Horizontal">
        <TextBox Width="410" Height="400" HorizontalAlignment="Left" Margin="30, 0, 0, 0"/>
        <Frame x:Name="Sheet" Source="GameSheet.xaml" Width="300" Height="400" Margin="30, 0, 0, 0"/>

    </StackPanel>
</Page>
